                <th>
                    @Html.DisplayNameFor(model => model.HelicalGearNumber)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.HelicalPinionNumber)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.HorizontalSpacerLength)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.VerticalSpacerLength)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.HorizontalGearCaseDeviation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HorizontalCarrierDeviation)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Bearing)
