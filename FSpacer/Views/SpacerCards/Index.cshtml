                <th>
                    @Html.DisplayNameFor(model => model.GearMount)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.VerticalMDGear)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.DriveNumber)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.OperatorID)
