            <td>
                @Html.DisplayFor(modelItem => item.GearMount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.VMDGear)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DriveNumber)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.OperatorID)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Active)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Backlash)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.HorizontalSetting)
            </td>
