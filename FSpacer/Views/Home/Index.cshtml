            @Html.DisplayNameFor(model => model.VerticalGearCaseDeviation)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.VerticalSpacerLength)
        </th>
    </tr>

    @foreach (var item in Model)
        {
            <tr>
                
                <th>
                   @Html.DisplayFor(modelItem => item.date)
                </th>
                <th>
                   @Html.DisplayFor(modelItem => item.Backlash)
                </th>
                <th>
                   @Html.DisplayFor(modelItem => item.Active)
                </th>
