        <th>
            @Html.DisplayNameFor(model => model.HorizontalSpacerLength)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OperatorID)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OutputSetting)
        </th>
