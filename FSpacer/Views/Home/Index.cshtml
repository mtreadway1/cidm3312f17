        <th>
            @Html.DisplayNameFor(model => model.Active)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Bearing)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.DriveNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GearMount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.GearNumber)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HorizontalMDGear)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.HelicalGearNumber)
        </th>
